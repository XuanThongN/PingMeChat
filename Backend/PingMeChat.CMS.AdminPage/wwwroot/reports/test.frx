		private void Table3_ManualBuild(object sender, EventArgs e)
		{
		// get the data source by its name

		DataSourceBase rowData = Report.GetDataSource(&quot;Relation&quot;);

		// initialize it

		rowData.Init();



		// printing the table header

		Table3.PrintRow(0);

		Table3.PrintColumns();



		// loop through the data source rows

		while (rowData.HasMoreRows)

		{

		// printing the table row

		Table3.PrintRow(1);

		Table3.PrintColumns();



		// select the next data row

		rowData.Next();

		}
		for(int i = rowData.RowCount; i &lt; 12; i++) {
		Table3.PrintRow(2);

		Table3.PrintColumns();
		}

		}



